<div>
	{{#with main}}
		<main>
			<section>
				{{> title data="My education" big=true}}
				<ul>
					{{#each education}}
						<li>{{name}}</li>
					{{#if isEnd}} (Finished) {{/if}}
					{{#unless isEnd}} (In process) {{/unless}}
					{{/each}}
				</ul>
			</section>
			<section>
                {{> title data="My certificates"}}
				<ul>
					{{#each certificates}} 
						<li>{{name}} ({{id}})</li>
					{{/each}}
				</ul>
			</section>
		</main>
	{{/with}}
</div>



