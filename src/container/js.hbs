<div>
  <h1>{{name}}</h1>
  <hr />
  <p>{{description}}</p>
  <hr />
  <h2>History</h2>
  <ul>
    <li>Year: {{history.year}}</li>
    <li>Founder: {{history.founder}}</li>
  </ul>
  <hr />

  <h2>Types</h2>
  <ul>
    {{#each types}}
      <li>{{this}}</li>
    {{/each}}
  </ul>
  <hr />

  <h2>Syntax</h2>
  <ul>
    <li>Variables: {{syntax.variables}}</li>
    <li>Functions: {{syntax.functions}}</li>
    <li>Conditionals: {{syntax.conditionals}}</li>
    <li>Loops: {{syntax.loops}}</li>
    <li>Classes: {{syntax.classes}}</li>
  </ul>
  <hr />

  <h2>Features</h2>
  <ul>
    {{#each features}}
      <li>{{this}}</li>
    {{/each}}
  </ul>
  <hr />

  <h2>Libraries</h2>
  <ul>
    {{#each libraries}}
      <li>{{this}}</li>
    {{/each}}
  </ul>
  <hr />

  <h2>Tools</h2>
  <ul>
    {{#each tools}}
      <li>{{this}}</li>
    {{/each}}
  </ul>
  <hr />

  <h2>Community</h2>
  <ul>
    {{#each community}}
      <li>{{name}} ({{type}})</li>
    {{/each}}
  </ul>

</div>